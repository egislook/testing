<div class="w-max sh-box-b-s1-l brd-xs-b brd-d-d bg-stickers clear">
    <div class="spc-lg brd-xs-b brd-b1 clear">
        <div class="col-4 left align-c">
            <a href="/">
                <div class="brd-xs brd-w">
                    <h1 class="txt-oswald txt-w">Go Big or Go Home.</h1>
                </div>
                <div class="txt-w txt-w spc-sm txt-xs">Our small betting community.</div>
            </a>
        </div>
        <div class="txt-oswald txt-w txt-md right col-2 align-r">
            {{? it.users}}
                {{for(var i in it.users) { }}
                    {{var pot = it.users[i].stats.win - it.users[i].stats.loss;}}
                    <a href="/{{=it.users[i].name}}">
                        <div class="spc-xs spc-md-l mrg-xs-b align-l txt-w" style="background:#000; opacity:0.8">
                            {{=it.users[i].name}}
                            <span class="{{=pot<0 ? 'txt-red' : 'txt-g'}} right">{{= pot.toFixed(2)}} $</span>
                        </div>
                    </a>
                {{ } }}
            {{?}}
            
        </div>
    </div>
</div>